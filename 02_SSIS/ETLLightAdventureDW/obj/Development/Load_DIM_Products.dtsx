                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=17.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT 
    pro.ProductID,
    pro.Name,
    pro.Color,
    pro.[Size],
    sub.Name AS SubcategoryName,
    cat.Name AS CategoryName
FROM Production.Product AS pro
INNER JOIN Production.ProductSubcategory AS sub
    ON pro.ProductSubcategoryID = sub.ProductSubcategoryID
INNER JOIN Production.ProductCategory AS cat
    ON sub.ProductCategoryID = cat.ProductCategoryID;</property>
